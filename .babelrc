{
  "plugins": [
    "inline-react-svg",
    "transform-class-properties",
    "transform-async-to-generator",
    "transform-es2015-classes"
  ],
  "env": {
    "development": {
      "presets": [
        ["env", { "targets": { "electron": "1.6.0" } }],
        "stage-0",
        "react"
      ],
      "plugins": [
        "react-hot-loader/babel"
      ],
      "sourceMaps": "inline"
    },
    "production": {
      "presets": [
        ["env", { "targets": { "electron": "1.6.0" } }],
        "react"
      ],
      "sourceMaps": "none"
    }
  }
}
